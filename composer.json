{
	"name": "wapplersystems/address",
	"type": "typo3-cms-extension",
	"description": "Address extension, based on extbase & fluid for TYPO3 CMS",
	"keywords": [
		"TYPO3",
		"extension",
		"address",
		"addresses"
	],
	"homepage": "https://wappler.systems",
	"authors": [
		{
			"name": "Sven Wappler",
			"role": "Developer",
			"homepage": "https://wappler.systems"
		}
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"typo3/cms-core": "^11.5.0"
	},
	"suggest": {
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8.0",
		"friendsofphp/php-cs-fixer": "^2.0",
		"nimut/testing-framework": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"WapplerSystems\\Address\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WapplerSystems\\Address\\Tests\\": "Tests"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "address"
		}
	}
}
