{
  "name": "wapplersystems/address",
  "type": "typo3-cms-extension",
  "description": "Versatile address extension, based on extbase & fluid for TYPO3 CMS",
  "keywords": [
    "TYPO3",
    "extension",
    "address",
    "addresses"
  ],
  "homepage": "http://wappler.systems",
  "authors": [
    {
      "name": "Sven Wappler",
      "role": "Developer",
      "homepage": "http://wappler.systems"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=8.5.0 <9"
  },
  "suggest": {
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "WapplerSystems\\Address\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WapplerSystems\\Address\\Tests\\": "Tests"
    }
  },
  "replace": {
    "address": "self.version",
    "typo3-ter/address": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
